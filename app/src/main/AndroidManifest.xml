<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.donation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Donation">
        <activity android:name=".UploadRequirements" android:parentActivityName=".ReceiverHomePage"/>
        <activity android:name=".ThankYou" android:parentActivityName=".UploadRequirements"/>
        <activity android:name=".ReceiverHomePage" android:parentActivityName=".OrgDetails"/>
        <activity android:name=".OrgDetails" android:parentActivityName=".MainActivity_receiver"/>
        <activity android:name=".MainActivity_receiver" android:parentActivityName=".MainActivity"/>
        <activity android:name=".MainActivity_donor5" android:parentActivityName=".MainActivity_donor4"/>
        <activity android:name=".MainActivity_donor4" android:parentActivityName=".MainActivity_donor1"/>
        <activity android:name=".MainActivity_donor3" android:parentActivityName=".MainActivity_donor1"/>
        <activity android:name=".MainActivity_donor2" android:parentActivityName=".MainActivity_donor1" />
        <activity android:name=".MainActivity_donor1" android:parentActivityName=".MainActivity" />
        <activity android:name=".accept_request" android:parentActivityName=".user1"/>
        <activity android:name=".decline_request" android:parentActivityName=".user1"/>

        <activity
            android:name=".user1"
            android:label="@string/title_activity_user1"
            android:parentActivityName=".donation_requests" />
        <activity android:name=".donation_requests" android:parentActivityName=".ReceiverHomePage"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>